{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "rootDir": ".",
    "paths": {
      "src/engine/arjuna-orm/*": [
        "packages/arjuna-server/src/engine/twenty-orm/*"
      ],
      "src/engine/core-modules/arjuna-config/*": [
        "packages/arjuna-server/src/engine/core-modules/twenty-config/*"
      ],
      "src/engine/workspace-manager/arjuna-standard-application/*": [
        "packages/arjuna-server/src/engine/workspace-manager/twenty-standard-application/*"
